{
  "name": "realmultillm",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "setup": "./scripts/setup.sh",
    "deploy": "./scripts/deploy.sh",
    "audit:security": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "benchmark": "node scripts/benchmark.js",
    "analyze": "cross-env ANALYZE=true npm run build",
    "preinstall": "npx only-allow npm",
    "deploy:check": "npm run type-check && npm run lint && npm run build",
    "deploy:netlify": "netlify deploy --prod --dir=.next",
    "db:reset": "prisma migrate reset",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "auth:rotate-secret": "node scripts/rotate-nextauth-secret.js"
    ,
    "hooks:install": "git config core.hooksPath .githooks",
    "hooks:uninstall": "git config --unset core.hooksPath || true"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
