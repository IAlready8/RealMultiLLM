{
  "name": "realmultillm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security && npm run test:performance && npm run test:deployment",
    "test:unit": "vitest run test/**/*.test.ts test/**/*.test.tsx",
    "test:integration": "vitest run test/integration/**/*.test.ts",
    "test:e2e": "playwright test",
    "test:security": "vitest run test/security/**/*.test.ts",
    "test:performance": "vitest run test/performance/**/*.test.ts",
    "test:deployment": "vitest run test/deployment/**/*.test.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "docker:build": "docker build -t realmultillm .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.local realmultillm",
    "docker:dev": "docker-compose up",
    "docker:down": "docker-compose down",
    "setup": "./scripts/setup.sh",
    "audit:security": "npm audit --audit-level moderate",
    "benchmark": "node scripts/benchmark.js",
    "native:setup": "npm install && npx prisma generate && npx prisma db push",
    "native:dev": "npm run dev",
    "native:build": "npm run build && npm run start"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.17.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "autoprefixer": "^10.4.19",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "lucide-react": "^0.417.0",
    "next": "14.2.5",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "postcss": "^8",
    "prisma": "^5.17.0",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^3.1.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/recharts": "^1.8.29",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "idb": "^8.0.0",
    "jsdom": "^24.1.1",
    "prettier": "^3.3.3",
    "typescript": "^5",
    "vitest": "^2.0.5"
  }
}
