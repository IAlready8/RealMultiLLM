{
  "name": "realmultillm",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:staged": "vitest run --coverage --reporter=verbose --changed",
    "profile": "node scripts/profile.js",
    "bundle-analyze": "ANALYZE=true npm run build",
    "security-audit": "npm audit --audit-level moderate",
    "clean": "rm -rf .next node_modules/.cache",
    "db:reset": "npx prisma db push --force-reset",
    "db:studio": "npx prisma studio",
    "db:generate": "npx prisma generate",
    "validate": "npm run lint:check && npm run type-check && npm run test:run && npm run build"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "5.10.2",
    "@radix-ui/react-alert-dialog": "1.1.14",
    "@radix-ui/react-avatar": "1.0.4",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-select": "2.0.0",
    "@radix-ui/react-slider": "1.1.2",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-toast": "1.1.5",
    "@types/bcryptjs": "2.4.6",
    "bcryptjs": "3.0.2",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "idb": "8.0.0",
    "lucide-react": "0.354.0",
    "next": "14.2.31",
    "next-auth": "4.24.7",
    "next-themes": "0.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "2.15.4",
    "tailwind-merge": "2.2.1",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.2.31",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "20.19.9",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.0",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "4.7.0",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.31",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "postcss": "8.5.1",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "prisma": "5.10.2",
    "tailwindcss": "3.4.15",
    "typescript": "5.6.3",
    "vitest": "3.2.4"
  }
}
